{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "https://api.megapot.io/api/v1/:path*"
    },
    {
      "source": "/rpc",
      "destination": "https://base.meowrpc.com/"
    },
    {
      "source": "/basescan-api/:path*",
      "destination": "https://api.basescan.org/api/:path*"
    },
    {
      "source": "/og/:id",
      "destination": "/api/og?id=:id"
    }
  ]
}